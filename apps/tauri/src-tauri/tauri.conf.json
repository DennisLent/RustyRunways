{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RustyRunways",
  "version": "3.0.3",
  "identifier": "com.rustyrunways.desktop",
  "build": {
    "devUrl": "http://localhost:5173",
    "frontendDist": "../ui/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "RustyRunways",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png",
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}
